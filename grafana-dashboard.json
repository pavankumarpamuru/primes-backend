{
  "title": "Primes Backend Monitoring",
  "uid": "primes-backend",
  "timezone": "browser",
  "refresh": "10s",
  "time": {"from": "now-1h", "to": "now"},
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count[5m]))",
        "legendFormat": "req/s"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps"}}
    },
    {
      "title": "Response Latency (p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0},
      "targets": [{
        "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_milliseconds_bucket[5m])) by (le)) / 1000",
        "legendFormat": "p95"
      }],
      "fieldConfig": {"defaults": {"unit": "s"}}
    },
    {
      "title": "HTTP Error Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_errors_total[5m])) / sum(rate(http_server_duration_milliseconds_count[5m]))",
        "legendFormat": "error rate"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "title": "Login Success Rate",
      "type": "stat",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
      "targets": [{
        "expr": "sum(rate(auth_attempts_total{success=\"true\"}[5m])) / sum(rate(auth_attempts_total[5m]))",
        "legendFormat": "success rate"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 0.8, "color": "yellow"},
              {"value": 0.95, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "title": "Database Query Latency (p95)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
      "targets": [{
        "expr": "histogram_quantile(0.95, rate(db_query_duration_milliseconds_bucket[5m]))",
        "legendFormat": "p95"
      }],
      "fieldConfig": {"defaults": {"unit": "ms"}}
    },
    {
      "title": "Requests by Status Code",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
      "targets": [{
        "expr": "sum by (http_status_code) (rate(http_server_duration_milliseconds_count[5m]))",
        "legendFormat": "{{http_status_code}}"
      }]
    },
    {
      "title": "CPU Usage",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
      "targets": [{
        "expr": "system_cpu_usage",
        "legendFormat": "CPU"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 70, "color": "yellow"},
              {"value": 85, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "title": "Memory Usage",
      "type": "gauge",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
      "targets": [{
        "expr": "system_memory_usage",
        "legendFormat": "Memory"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 75, "color": "yellow"},
              {"value": 90, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "title": "Average Response Time by Endpoint",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
      "targets": [{
        "expr": "sum by (http_target) (rate(http_server_duration_milliseconds_sum[5m])) / sum by (http_target) (rate(http_server_duration_milliseconds_count[5m])) / 1000",
        "legendFormat": "{{http_target}}"
      }],
      "fieldConfig": {"defaults": {"unit": "s"}}
    },
    {
      "title": "Prime Generation Requests",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [{
        "expr": "sum(rate(primes_requests_total[5m]))",
        "legendFormat": "req/s"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps"}}
    },
    {
      "title": "Prime Generation Latency (p50/p95/p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(primes_generation_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(primes_generation_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(primes_generation_duration_milliseconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "ms"}}
    }
  ]
}
